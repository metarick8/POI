        "/api/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Authenticate user",
                "operationId": "a3b306d14572d1f4bd6c064b3233e7b8",
                "requestBody": {
                    "description": "Login credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "jad@email.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "12345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login successful"
                    },
                    "401": {
                        "description": "Invalid credentials"
                    }
                }
            }
        },
        "/api/profile": {
            "get": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Get authenticated user profile",
                "description": "Returns profile details based on the token (User, Coach, Debater, Judge)",
                "operationId": "d32e56b6274eb68e980da6d91fbe9fd8",
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "oneOf": [
                                        {
                                            "schema": "User",
                                            "required": [
                                                "id",
                                                "first_name",
                                                "last_name",
                                                "email"
                                            ],
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "first_name": {
                                                    "type": "string",
                                                    "example": "Jad"
                                                },
                                                "last_name": {
                                                    "type": "string",
                                                    "example": "Alhalabi"
                                                },
                                                "email": {
                                                    "type": "string",
                                                    "example": "jadalhalabi88@gmail.com"
                                                },
                                                "profile_picture": {
                                                    "type": "string",
                                                    "example": "random link"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        {
                                            "schema": "Coach",
                                            "required": [
                                                "id",
                                                "first_name",
                                                "last_name",
                                                "email",
                                                "team"
                                            ],
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 10
                                                },
                                                "first_name": {
                                                    "type": "string",
                                                    "example": "Coach Name"
                                                },
                                                "last_name": {
                                                    "type": "string",
                                                    "example": "Last Name"
                                                },
                                                "email": {
                                                    "type": "string",
                                                    "example": "coach@email.com"
                                                },
                                                "team": {
                                                    "type": "array",
                                                    "items": {
                                                        "properties": {
                                                            "first_name": {
                                                                "type": "string",
                                                                "example": "Debater first name"
                                                            },
                                                            "last_name": {
                                                                "type": "string",
                                                                "example": "Debater last name"
                                                            },
                                                            "email": {
                                                                "type": "string",
                                                                "example": "debater@email.com"
                                                            },
                                                            "profile_picture": {
                                                                "type": "string",
                                                                "example": "random link"
                                                            }
                                                        },
                                                        "type": "object"
                                                    }
                                                }
                                            },
                                            "type": "object"
                                        },
                                        {
                                            "schema": "Debater",
                                            "required": [
                                                "id",
                                                "first_name",
                                                "last_name",
                                                "email",
                                                "coach_name"
                                            ],
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 20
                                                },
                                                "first_name": {
                                                    "type": "string",
                                                    "example": "Debater Name"
                                                },
                                                "last_name": {
                                                    "type": "string",
                                                    "example": "Last Name"
                                                },
                                                "email": {
                                                    "type": "string",
                                                    "example": "debater@email.com"
                                                },
                                                "profile_picture": {
                                                    "type": "string",
                                                    "example": "random link"
                                                },
                                                "coach_name": {
                                                    "type": "string",
                                                    "example": "Coach first name + Coach last name"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized - Invalid or missing token"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                },
                "security": [
                    {
                        "BearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "BearerAuth": {
                "type": "http",
                "description": "Enter your JWT token in the format: Bearer {token}",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "Authentication",
            "description": "Authentication"
        }
    ]
