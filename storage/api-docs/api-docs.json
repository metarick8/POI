    "paths": {
        "/api/register/user": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Add user",
                "description": "Add new mobile user to the application",
                "operationId": "878228d90ce6296c71ee74e3a4c84132",
                "requestBody": {
                    "description": "User register credentials",
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "first_name",
                                    "last_name",
                                    "email",
                                    "password",
                                    "password_confirmation",
                                    "profile_picture"
                                ],
                                "properties": {
                                    "first_name": {
                                        "type": "string",
                                        "example": "Jad"
                                    },
                                    "last_name": {
                                        "type": "string",
                                        "example": "Alhalabi"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "jad@email.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "12345678"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "example": "12345678"
                                    },
                                    "profile_picture": {
                                        "description": "Profile picture file upload",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User has been created!"
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "401": {
                        "description": "Invalid credientials"
                    },
                    "422": {
                        "description": "Unprocessable Content"
                    }
                }
            }
        },
